    "version": "0.2.0",
    "configurations": [
        {
            "name": "Search interactively",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "args": [
                "-d",
                "search",
                "-i"
            ],
        },
        {
            "name": "Search w/ api URL",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "args": [
                "-d",
                "--api-url",
                "https://matadev.cps.pf/api",
                "-u",
                "01846",
                "-p",
                "33p@SSword",
                "search",
            ],
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": [],
            "showLog": true
        }
    ]
}
